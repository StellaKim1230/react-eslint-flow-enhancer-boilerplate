{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "arrow-parens": 0,
    "react/jsx-filename-extension": 0,
    "react/destructuring-assignment": 0,
    "react/no-access-state-in-setstate": 0,
    "react/button-has-type": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 0,
    "quotes": 2,
    "semi": [
      2,
      "never"
    ],
    "no-unused-vars": 0,
    "no-plusplus": 0,
    "function-paren-newline": 0,
    "no-console": 0
  }
}